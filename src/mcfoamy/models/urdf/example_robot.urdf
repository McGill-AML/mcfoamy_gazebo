<?xml version="1.0" ?>
<robot name="example_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="plane_urdf">
    <inertial>
      <mass value="1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://gazebo_example/meshes/plane.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://gazebo_example/meshes/plane.dae"/>
      </geometry>
    </collision>
  </link>
</robot>
