<?xml version="1.0"?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>
    <scene>
      <shadows>0</shadows>
    </scene>


    <model name='Wall'>
      <static>true</static>
      <link name="wall1">
        <pose>30 0 0 0 0 0 </pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 80 40</size>
            </box>
          </geometry>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 80 40</size>
            </box>
          </geometry>
          <max_contacts>10</max_contacts>
        </collision>
      </link>
      <link name="wall2">
        <pose>-30 0 0 0 0 0 </pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 80 40</size>
            </box>
          </geometry>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 80 40</size>
            </box>
          </geometry>
          <max_contacts>10</max_contacts>
        </collision>
      </link>
      <link name="wall3">
        <pose>0 -40 0 0 0 1.57 </pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 60 40</size>
            </box>
          </geometry>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 60 40</size>
            </box>
          </geometry>
          <max_contacts>10</max_contacts>
        </collision>
      </link>
      <link name="wall4">
        <pose>0 40 0 0 0 1.57 </pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 60 40</size>
            </box>
          </geometry>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 60 40</size>
            </box>
          </geometry>
          <max_contacts>10</max_contacts>
        </collision>
      </link>
    </model>

    <model name='Wall_Obstacles'>
      <static>true</static>
      <link name="wall1">
        <pose>5 -10 0 0 0 1.57 </pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 50 40</size>
            </box>
          </geometry>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 50 40</size>
            </box>
          </geometry>
          <max_contacts>10</max_contacts>
        </collision>
      </link>
      <link name="wall2">
        <pose>-5 0 0 0 0 1.57 </pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 50 40</size>
            </box>
          </geometry>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 50 40</size>
            </box>
          </geometry>
          <max_contacts>10</max_contacts>
        </collision>
      </link>
    </model>


    <!-- Physics settings -->
    <physics name='default_physics' default='0' type='ode'>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>.25</real_time_factor>
      <real_time_update_rate>250</real_time_update_rate>
    </physics>

    <gui>
        <camera name="user_camera">
            <pose>-20 -10 100 0 1.4 0</pose>
        </camera>
    </gui>
  </world>
</sdf>
